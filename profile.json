//minimal emulator example
{
  //Name of the Profile
  "Dolphin": {
    //The executable of the emulator
    "exe": "D:\\Software\\Dolphin\\Dolphin.exe",
    //Where the ROMs are
    "dir": "D:\\Games\\Wii",
    //The command so that the emulator ($e) opens the ROM-File ($f)
    "command": "$e --batch --exec=$f"
  }
}
